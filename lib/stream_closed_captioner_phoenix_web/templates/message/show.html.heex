<h1>Show Message</h1>

<ul>

  <li>
    <strong>Transcript:</strong>
    <%= @message.transcript_id %>
  </li>

  <li>
    <strong>Text:</strong>
    <%= @message.text %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.transcript_message_path(@conn, :edit, @message.transcript_id, @message) %></span>
<span><%= link "Back", to: Routes.transcript_path(@conn, :show, @message.transcript_id) %></span>
