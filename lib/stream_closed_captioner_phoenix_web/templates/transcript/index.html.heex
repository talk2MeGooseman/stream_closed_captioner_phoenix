<h1>Listing Transcripts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th>Session</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for transcript <- @transcripts do %>
    <tr>
      <td><%= transcript.user_id %></td>
      <td><%= transcript.name %></td>
      <td><%= transcript.session %></td>

      <td>
        <span><%= link "Show", to: ~p"/transcripts/#{transcript}" %></span>
        <span><%= link "Edit", to: ~p"/transcripts/#{transcript}/edit" %></span>
        <span><%= link "Delete", to: ~p"/transcripts/#{transcript}", method: :delete, data: [confirm: "Are you sure?"] %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>
